/**
 * Test for designation type row1.
 *
 * @param p_count The number of rows selected.
 *                * 0 For a invalid test.
 *                * 1 For a valid test.
 *                * 2 For a invalid test.
 */
create procedure dbo.tst_test_rows
  @p_count int
-- type: rows
as
  select *
  from   TST_FOO2
  where  tst_c00 <= @p_count;
